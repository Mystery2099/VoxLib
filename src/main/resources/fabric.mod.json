{
	"schemaVersion": 1,
	"id": "voxlib",
	"version": "${version}",
	"name": "Mystery2099's VoxLib",
	"description": "A library that provides utilities for manipulating, creating, and rotating voxel shapes in Minecraft modding, with performance optimizations and common shape patterns.",
	"authors": [
		"Mystery2099"
	],
	"contact": {
		"homepage": "https://github.com/Mystery2099/VoxLib",
		"sources": "https://github.com/Mystery2099/VoxLib",
		"issues": "https://github.com/Mystery2099/VoxLib/issues"
	},
	"license": "MMPL-1.0.1",
	"icon": "assets/voxlib/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "com.github.mystery2099.voxlib.VoxLib",
				"adapter": "kotlin"
			}
		]
	},
	"depends": {
		"fabricloader": ">=0.18.4",
		"minecraft": "~1.19.4",
		"java": ">=17",
		"fabric-api": ">=0.87.2+1.19.4",
		"fabric-language-kotlin": ">=1.13.8+kotlin.2.3.0"
	},
	"custom": {
		"modmenu": {
			"badges": [
				"library"
			]
		}
	}
}